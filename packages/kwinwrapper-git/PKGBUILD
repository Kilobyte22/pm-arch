pkgname=kwinwrapper-git
pkgver=1
pkgrel=2
pkgdesc="Simple script to start plasma on Arch Linux"
arch=('i686' 'x86_64')
url="https://community.kde.org/Plasma/Mobile"
license=('GPL3')
source=('kwinwrapper')
md5sums=('7cddd644627f71863f8670ff48caffe2')

package() {
    install -Dm755 kwinwrapper ${pkgdir}/usr/bin/kwinwrapper
}
